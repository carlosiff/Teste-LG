<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="br.edu.iff.pooa20162.teste_lg.activity.MainActivity">

    <Button
        android:text="Veículos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/veiculo"
        android:onClick="chamaTelaCadastrov"
        android:layout_marginTop="125dp"
        android:layout_below="@+id/proprietario"
        android:layout_centerHorizontal="true"
        tools:ignore="HardcodedText" />

    <Button
        android:text="Proprietários"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="94dp"
        android:id="@+id/proprietario"
        android:onClick="chamaTelaCadastrop"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        tools:ignore="HardcodedText" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastro de Veículos e Propietários!"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        tools:ignore="HardcodedText" />

</RelativeLayout>